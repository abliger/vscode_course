{
	"name": "rollup-course",
	"displayName": "rollup-course",
	"description": "rollup 学习演示",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.63.0"
	},
	"icon": "public/Snipaste_2021-11-18_14-14-45.png",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:aaa",
		"onView:package-dependencies",
		"onNotebook:my-notebook"
	],
	"main": "./out/extension.js",
	"contributes": {
		"title": "course",
		"commands": [
			{
				"command": "aaa",
				"title": "aaa"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "package-explorers",
					"title": "前端演示学习",
					"icon": "public/Snipaste_2021-11-18_14-14-45.png"
				}
			]
		},
		"views": {
			"package-explorers": [
				{
					"id": "package-dependencies",
					"name": "test"
				}
			]
		},
		"notebooks": [
			{
				"id": "my-notebook",
				"type": "my-notebook",
				"displayName": "My Notebook",
				"selector": [
					{
						"filenamePattern": "*.notebook"
					}
				]
			}
		],
		"walkthroughs": [
			{
				"id": "rollup_learn",
				"title": "%rollup-course.walkthroughs.title%",
				"description": "%rollup-course.walkthroughs.description%",
				"steps": [
					{
						"id": "quick_start",
						"title": "%rollup-course.walkthroughs.quick_start.title%",
						"description": "%rollup-course.walkthroughs.quick_start.description%",
						"media": {
							"markdown": "public/rollup/test.md"
						}
					}
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint --fix src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/shelljs": "^0.8.11",
		"@types/vscode": "^1.63.0",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"@vscode/test-electron": "^2.0.3",
		"eslint": "^8.6.0",
		"eslint-config-standard": "latest",
		"eslint-plugin-html": "^6.2.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^4.2.1 || ^5.0.0",
		"eslint-plugin-vue": "^8.2.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"typescript": "^4.5.4"
	}
}